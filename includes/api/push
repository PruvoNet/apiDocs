  </span><span class="s2">"hotelName"</span><span class="p">:</span><span class="w"> </span><span class="s2">"Main Hotel Budapest"</span><span class="p">,</span><span class="w">
  </span><span class="s2">"mealPlan"</span><span class="p">:</span><span class="w"> </span><span class="mi">1</span><span class="p">,</span><span class="w">
  </span><span class="s2">"address"</span><span class="p">:</span><span class="w"> </span><span class="s2">"24 Main street"</span><span class="p">,</span><span class="w">
