  </span><span class="s2">"arrivalDate"</span><span class="p">:</span><span class="w"> </span><span class="s2">"05/25/2020"</span><span class="p">,</span><span class="w">
  </span><span class="s2">"departureDate"</span><span class="p">:</span><span class="w"> </span><span class="s2">"05/27/2020"</span><span class="p">,</span><span class="w">
  </span><span class="s2">"lastFreeCancelTime"</span><span class="p">:</span><span class="w"> </span><span class="s2">"05/21/2020"</span><span class="p">,</span><span class="w">
  </span><span class="s2">"hotelName"</span><span class="p">:</span><span class="w"> </span><span class="s2">"Main Hotel Budapest"</span><span class="p">,</span><span class="w">
